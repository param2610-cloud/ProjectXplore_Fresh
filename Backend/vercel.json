{
  "version": 2,
  "builds": [
    {
      "src": "prisma/schema.prisma",
      "use": "@prisma/cli"
    },
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.js"
    }
  ],
  "env": {
    "DATABASE_URL": "@database_url"
  }
}