{
  "version": 2,
  "builds": [
    {
      "src": "prisma/schema.prisma",
      "use": "@prisma/cli"
    },
    {
      "src": "/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "DATABASE_URL": process.env.DATABASE_URL
  }
}
